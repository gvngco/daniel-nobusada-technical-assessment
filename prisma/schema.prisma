generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = "file:./dev.db"
}

model User {
    id   String @id @default(cuid())
    name String @unique
    todoList Todo[]
}

model Todo {
    id String @id
    user User @relation(fields: [userId], references:[id])
    userId String
    value String
    completed Boolean @default(false)
}